{
  "name": "serverless-aws-lambda",
  "version": "1.0.0",
  "description": "Lambda function for getting customer data with Clean Architecture",
  "main": "dist/handler.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "deploy": "npm run build && serverless deploy",
    "deploy:dev": "npm run build && serverless deploy --stage dev",
    "deploy:prod": "npm run build && serverless deploy --stage prod",
    "remove": "serverless remove",
    "logs": "serverless logs -f getCustomer",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "typescript",
    "clean-architecture",
    "customers"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "serverless": "^3.38.0",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-jest": "^29.4.1",
    "typescript": "^5.1.6"
  }
}
